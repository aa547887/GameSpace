@model GameSpace.Areas.MemberManagement.Models.ManagerRoleEntry

<h2>刪除角色</h2>

<h4>你確定要刪除此筆資料嗎？</h4>
<div>
    <dl class="row">
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.ManagerId)</dt>
        <dd class="col-sm-10">@Model.ManagerId</dd>
        <dt class="col-sm-2">@Html.DisplayNameFor(m => m.ManagerRoleId)</dt>
        <dd class="col-sm-10">@Model.ManagerRoleId</dd>
    </dl>
</div>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" asp-for="ManagerId" />
    <input type="hidden" asp-for="ManagerRoleId" />
    <button type="submit" class="btn btn-danger">刪除</button> |
    <a asp-action="Index">返回清單</a>
</form>
