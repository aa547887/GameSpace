@model GameSpace.Areas.MemberManagement.Models.ManagerRoleEntry

<h2>新增角色</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label asp-for="ManagerId"></label>
        <input asp-for="ManagerId" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ManagerRoleId"></label>
        <input asp-for="ManagerRoleId" class="form-control" />
    </div>
    
    <button type="submit" class="btn btn-primary">建立</button> |
    <a asp-action="Index">返回清單</a>
</form>
